<UserControl x:Class="SRTP2021.ViewUI.SubCon_AllView.Sub_DashBoard"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SRTP2021.ViewUI.SubCon_AllView"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Viewbox>
        <Grid>
            <Image Source="..\..\Assets\Images\dashBoard.png"  Width="75"/>
            <Image Source="..\..\Assets\Images\dashBoardPoint.png" Width="60" Height="60" Name="dashPoint">
                <Image.RenderTransform>
                    <RotateTransform Angle="151" CenterX="30" CenterY="30"/>
                </Image.RenderTransform>
            </Image>
        </Grid>
    </Viewbox>
</UserControl>
